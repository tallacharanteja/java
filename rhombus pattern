public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		
	int row=6;
		for(int i=1;i<=row;i++){
		    
		    for(int j=1;j<=row-i;j++){
		        System.out.print(" ");
		    }
		    int n;
		    for(n=1;n<=row;n++){
		        System.out.print("* ");
		    }
		    
		    
		  System.out.println();
		}
	}
}
